@article{GOMES2018,
title = {Enabling rootless Linux Containers in multi-user environments: The udocker tool},
journal = {Computer Physics Communications},
volume = {232},
pages = {84-97},
year = {2018},
issn = {0010-4655},
doi = {10.1016/j.cpc.2018.05.021},
