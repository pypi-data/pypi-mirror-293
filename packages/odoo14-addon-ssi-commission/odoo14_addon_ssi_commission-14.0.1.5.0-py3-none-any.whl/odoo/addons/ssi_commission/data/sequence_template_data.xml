<odoo>
<data noupdate="1">
<record id="sequence_template_commission" model="sequence.template">
    <field name="name">Standard</field>
    <field name="model_id" ref="model_commission" />
    <field name="sequence" eval="100" />
    <field name="initial_string">/</field>
    <field
                name="sequence_field_id"
                search="[('model_id.model','=','commission'),('name','=','name')]"
            />
    <field
                name="date_field_id"
                search="[('model_id.model','=','commission'),('name','=','date')]"
            />
    <field name="computation_method">use_python</field>
    <field name="python_code">result=True</field>
    <field name="sequence_id" ref="sequence_commission" />
    <field name="sequence_selection_method">use_sequence</field>
    <field name="add_custom_prefix" eval="0" />
    <field name="add_custom_suffix" eval="0" />
</record>

<record id="sequence_template_marketing_commission" model="sequence.template">
    <field name="name">Standard</field>
    <field name="model_id" ref="model_marketing_commission" />
    <field name="sequence" eval="100" />
    <field name="initial_string">/</field>
    <field
                name="sequence_field_id"
                search="[('model_id.model','=','marketing_commission'),('name','=','name')]"
            />
    <field
                name="date_field_id"
                search="[('model_id.model','=','marketing_commission'),('name','=','date')]"
            />
    <field name="computation_method">use_python</field>
    <field name="python_code">result=True</field>
    <field name="sequence_id" ref="sequence_marketing_commission" />
    <field name="sequence_selection_method">use_sequence</field>
    <field name="add_custom_prefix" eval="0" />
    <field name="add_custom_suffix" eval="0" />
</record>
</data>
</odoo>
