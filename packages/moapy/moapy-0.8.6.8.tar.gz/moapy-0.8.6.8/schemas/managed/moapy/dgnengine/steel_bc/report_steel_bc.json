{
  "openapi": "3.1.0",
  "info": {
    "title": "moapy",
    "description": "Schema for moapy",
    "version": "0.1.0"
  },
  "servers": [
    {
      "url": "https://moa.rpm.kr-dv-midasit.com/backend/function-executor/python-execute/"
    },
    {
      "url": "https://moa.rpm.kr-st-midasit.com/backend/function-executor/python-execute/"
    }
  ],
  "paths": {
    "/moapy/dgnengine/steel_bc/report_steel_bc": {
      "post": {
        "summary": "Report Steel Bc",
        "operationId": "report steel bc",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/report_steel_bc"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResultReport"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "MemberForce": {
        "properties": {
          "Nz": {
            "type": "number",
            "title": "Nz",
            "description": "Axial force",
            "default": 0.0
          },
          "Mx": {
            "type": "number",
            "title": "Mx",
            "description": "Moment about x-axis",
            "default": 0.0
          },
          "My": {
            "type": "number",
            "title": "My",
            "description": "Moment about y-axis",
            "default": 0.0
          },
          "Vx": {
            "type": "number",
            "title": "Vx",
            "description": "Shear about x-axis",
            "default": 0.0
          },
          "Vy": {
            "type": "number",
            "title": "Vy",
            "description": "Shear about y-axis",
            "default": 0.0
          }
        },
        "type": "object",
        "title": "Member Force",
        "description": "Member Force",
        "dataclassname": "moapy.data_pre.MemberForce"
      },
      "ReportType": {
        "properties": {
          "type": {
            "type": "string",
            "enum": [
              "Text",
              "Html"
            ],
            "title": "Type",
            "description": "Report type",
            "default": "Text"
          }
        },
        "type": "object",
        "title": "Report Type",
        "dataclassname": "moapy.data_pre.ReportType"
      },
      "ResultReport": {
        "properties": {
          "type": {
            "type": "string",
            "title": "Type",
            "description": "Report Type",
            "default": "Text"
          },
          "contents": {
            "type": "string",
            "title": "Contents",
            "description": "contents",
            "default": ""
          }
        },
        "type": "object",
        "title": "Report",
        "description": "Report result",
        "dataclassname": "moapy.data_post.ResultReport"
      },
      "SteelDBaseMaterial": {
        "properties": {
          "code": {
            "type": "string",
            "title": "Code",
            "description": "Material Code",
            "default": "KS18(S)"
          },
          "name": {
            "type": "string",
            "title": "Name",
            "description": "Material Name",
            "default": "SS275"
          }
        },
        "type": "object",
        "title": "Steel DB Material",
        "description": "Steel DB Material",
        "dataclassname": "moapy.data_pre.SteelDBaseMaterial"
      },
      "SteelDBaseSection": {
        "properties": {
          "shape": {
            "type": "string",
            "title": "Shape",
            "description": "Shape",
            "default": "H"
          },
          "name": {
            "type": "string",
            "title": "Name",
            "description": "Section Name",
            "default": "H 400x200x8/13"
          }
        },
        "type": "object",
        "title": "Steel DB Section",
        "description": "Steel DB Section",
        "dataclassname": "moapy.data_pre.SteelDBaseSection"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      },
      "report_steel_bc": {
        "properties": {
          "matl": {
            "$ref": "#/components/schemas/SteelDBaseMaterial"
          },
          "sect": {
            "$ref": "#/components/schemas/SteelDBaseSection"
          },
          "load": {
            "$ref": "#/components/schemas/MemberForce"
          },
          "rptType": {
            "$ref": "#/components/schemas/ReportType"
          }
        },
        "type": "object",
        "required": [
          "matl",
          "sect",
          "load",
          "rptType"
        ],
        "title": "report steel bc"
      }
    }
  }
}